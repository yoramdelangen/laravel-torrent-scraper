{
  "name": "yoramdelangen/laravel-torrent-scraper",
  "description": "Provide an abstraction to seach for torrent files on many websites, and cloudflare by-pass.",
  "keywords": [
    "torrent",
    "piratebay",
    "kickass",
    "torrentz2",
    "eztv"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yoramdelangen/laravel-torrent-scraper",
  "authors": [
    {
      "name": "Fernando Carletti",
      "email": "contato@fernandocarletti.net",
      "homepage": "http://fernandocarletti.net",
      "role": "Developer"
    },
    {
      "name": "Yoram de Langen",
      "email": "yoramdelangen@gmail.com",
      "homepage": "https://yoramdelangen.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "~6.2",
    "symfony/dom-crawler": ">=2.6",
    "symfony/css-selector": ">=2.6",
    "tunaabutbul/cloudflare-middleware": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.2.*",
    "symfony/var-dumper": ">=2.6",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Yoramdelangen\\TorrentScraper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yoramdelangen\\TorrentScraper\\": "tests"
    }
  }
}
